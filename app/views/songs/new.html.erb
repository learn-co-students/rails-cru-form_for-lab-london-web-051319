<br>
<%= form_for :song, url: {action: "create"} do |f| %>
  <%= label :name, "Song Name:" %>
  <%= f.text_field :name %><br><br>
  <%= label :artist_id, "Artist ID:" %>
  <%= f.text_area :artist_id %>
  <%= label :genre_id, "Genre ID:" %>
  <%= f.text_area :genre_id %>
  <%= f.submit "Create Song" %>
<% end %>